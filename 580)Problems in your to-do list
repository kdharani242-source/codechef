//c
#include <stdio.h>

int main() {
	// your code goes here
	int t;
	scanf("%d",&t);
	while(t--){
	    int n;
	    scanf("%d",&n);
	    int arr[n];
	    int count=0;
	    for(int i=0;i<n;i++){
	        scanf("%d",&arr[i]);
	        if(arr[i]>=1000){
	            count+=1;
	        }
	    }
	    printf("%d\n",count);
	    
	}

}

//python
# cook your dish here
def solve():
    count=0
    n=int(input())
    l=list(map(int,input().split()))
    for i in range(n):
        if(l[i]>=1000):
            count+=1
    print(count)
t=int(input())
for j in range(t):
    solve()
