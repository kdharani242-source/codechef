//c
#include <stdio.h>

int main() {
	// your code goes here
	int t;
	scanf("%d",&t);
	while(t--){
	    int n;
	    scanf("%d",&n);
	    int arr[n];
	    int count=0;
	    for(int i=0;i<n;i++){
	        scanf("%d",&arr[i]);
	        if(arr[i]%2!=0){
	            count+=1;
	        }
	    }
	    if(count%2==0){
	        printf("yes\n");
	    }
	    else{
	        printf("no\n");
	    }
	}

}

//python
# cook your dish here
def solve():
    n=int(input())
    li=list(map(int,input().split()))
    count=0
    for i in range(n):
        if li[i]%2!=0:
            count+=1
    if(count%2==0):
        print("yes")
    else:
        print("no")
t=int(input())
for k in range(t):
    solve()
