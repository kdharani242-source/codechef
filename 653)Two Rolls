//c
#include <stdio.h>

int main() {
	// your code goes here
	int t;
    scanf("%d", &t);
    while (t--) {
        int x, y;
        scanf("%d %d", &x, &y);
        if ((x + (2 * y)) <= 50 && 50 <= (x + (2 * y) + 10)) {
            printf("Yes\n");
        } else {
            printf("No\n");
        }
    }

}

//python
# cook your dish here
t=int(input())
for i in range(t):
    x,y=map(int,input().split())
    if(x+(2*y)<=50 and x+(2*y)+10>=50):
        print("yes")
    else:
        print("no")
