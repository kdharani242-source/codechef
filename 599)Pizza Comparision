//c
#include <stdio.h>

int main() {
    int t;
    scanf("%d", &t);
    while (t--) {
        int a, b;
        scanf("%d %d", &a, &b);
        
        double small_ratio = 100.0 / a;
        double large_ratio = 225.0 / b;
        
        if (small_ratio > large_ratio) {
            printf("Small\n");
        } else if (small_ratio == large_ratio) {
            printf("Equal\n");
        } else {
            printf("Large\n");
        }
    }
    return 0;
}
//python
# cook your dish here
t=int(input())
for i in range(t):
    a,b=map(int,input().split())
    if(100/a>225/b):
        print("small")
    elif(100/a==225/b):
        print("equal")
    else:
        print("large")
