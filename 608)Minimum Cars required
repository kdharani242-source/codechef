//c
#include <stdio.h>

int main() {
	// your code goes here
	int t;
	scanf("%d",&t);
	while(t--){
	    int x;
	    scanf("%d",&x);
	    if(x%4==0){
	        printf("%d\n",x/4);
	    }
	    else{
	        printf("%d\n",(x/4)+1);
	    }
	}

}

//python
# cook your dish here
t=int(input())
for i in range(t):
    x=int(input())
    if(x%4==0):
        print(x//4)
    else:
        print((x//4)+1)
