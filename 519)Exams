//c
#include <stdio.h>

int main() {
	// your code goes here
	int t;
	scanf("%d",&t);
	while(t--){
	    int x,y,z;
	    scanf("%d %d %d",&x,&y,&z);
	    if((x*y)/2<z){
	    printf("yes\n");
	}
	else{
	    printf("no\n");
	}
	}

}

//python
t=int(input())
for i in range(t):
    x,y,z=map(int,input().split())
    if((x*y)//2<z):
        print("yes")
    else:
        print("no")
