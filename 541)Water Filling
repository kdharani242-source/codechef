//c
#include <stdio.h>

int main() {
	// your code goes here
	int t;
	scanf("%d",&t);
	while(t--){
	    int a,b,c;
	    scanf("%d %d %d",&a,&b,&c);
	    if(a+b+c>=2){
	        printf("not now\n");
	    }
	    else{
	        printf("water filling time\n");
	    }
	}

}

//python
# cook your dish here
t=int(input())
for i in range(t):
    b1,b2,b3=map(int,input().split())
    if(b1+b2+b3>=2):
        print("not now")
    else:
        print("water filling time")
