//c
#include <stdio.h>

int main() {
	// your code goes here
	int t;
	scanf("%d",&t);
	while(t--){
	    int n;
	    scanf("%d",&n);
	    if(n==1 || n%2==0){
	        printf("yes\n");
	    }
	    else{
	        printf("no\n");
	    }
	}

}

//python
# cook your dish here
t=int(input())
for i in range(t):
    n=int(input())
    if(n==1 or n%2==0):
        print('yes')
    else:
        print('no')
